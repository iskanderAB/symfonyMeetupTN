{
  admin off
}

localhost:3001 {
  mercure {
    publisher_jwt !ChangeThisMercureHubJWTSecretKey!
    subscriber_jwt !ChangeThisMercureHubJWTSecretKey!
    cors_origins *
    publish_origins *
    anonymous
    subscriptions
    demo
    compress off
	debug
    ui
  }
  respond "Mercure Hub is running :rocket:" 200
}